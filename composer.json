{
  "name": "richardtmiles/c6",
  "description": "The best MVC framework",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "vendor-dir": "vendor"
  },
  "autoload": {
    "psr-4": {
      "App\\":"Application/",
      "View\\": "Application/View/",
      "Controller\\": "Application/Controller/",
      "Model\\": "Application/Model/",
      "Tables\\": "Application/Tables/"
    }
  },
  "authors": [
    {
      "name": "Richard Miles",
      "email": "Richard@Miles.Systems"
    }
  ],

  "require-dev": {
    "google/cloud-tools": "^0.6"
  },

  "require": {
    "php": "^7.1",
    "google/cloud": "^0.21",
    "richardtmiles/carbonphp": "dev-StatsCoach#590d33f73b5d940b382de14b8f757c3febb157d4-dev",
    "google/apiclient": "^2.0",
    "facebook/graph-sdk": "^5.5",
    "ext-json": "*"
  }

}